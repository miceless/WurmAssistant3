<Application x:Class="AldursLab.WurmAssistant3.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"

             xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit"
             xmlns:wurmAssistant3="clr-namespace:AldursLab.WurmAssistant3"
             xmlns:infrastructure="clr-namespace:AldursLab.WurmAssistant3.Infrastructure">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary>
                    <wurmAssistant3:AppBootstrapper x:Key="Bootstrapper" />
                </ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
